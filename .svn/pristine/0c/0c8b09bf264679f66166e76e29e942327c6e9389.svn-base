
	.body{
		margin: auto;
	}
	.error{
		color:red;
		font-size:14px;
	}
	.textStyle{
		display:inline-block;
		height: 31px;
		width:250px;
		padding-left:6px;
		font-size: 14px;
		line-height: 1.42857143;
		color: #555;
		background-color: #fff;
		background-image: none;
		border: 1px solid #ccc;
		border-radius: 4px;
		transition: border-color ease-in-out .15s,box-shadow ease-in-out .15s;
		box-shadow: inset 0 1px 1px rgba(0,0,0,.075);
	}
	.tab{
		font-size: 12px;
		width: 800px;
		text-align: center;
		line-height: 30px;
		margin-top: 20px;
		margin-bottom: 20px;
	}
	.tab>tbody>tr>td{
		text-align: center;
	}
	.td1{
		width:150px;
	}
	.contain{
		z-index: 2;
		position: relative;
	}
	.showText{
		position:fixed;
		display: none;
		background-color:ghostwhite;
		z-index: 3
	}
	.title{
		display:inline-block;
		width: 1000px;
	    height: 40px;
	    line-height: 40px;
	    font-size: 18px;
	    font-weight: 600;
	    background-color: #F2F2F2;
	    padding-left: 20px;
	    margin-bottom: 20px;
	}
	.btn{
		display: block;
		height: 32px;
		width:86px;
		background-color:#26BF8C;
		border: 1px solid #ccc;
		color:white;
		margin-top: 20px;
		position: relative;
		left:32%;
		text-align: center;
		line-height: 32px;
	}
	span{
		font-size: 14px;
		position: relative;
		left:400px;
	}
