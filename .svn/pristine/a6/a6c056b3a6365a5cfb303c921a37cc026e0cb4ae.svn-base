@CHARSET "UTF-8";


.dialog{
	width: 600px;
    height: 400px;
    position: absolute;
    left:250px;
    top:150px;
	z-index: 9999;
}

.dialog_title{
	width:90%;
	height:50px;
	background-color: #F8F8F8;
	line-height: 50px;
	font-size: 20px;
	font-weight: 600;
	padding-left: 60px;
}

.dialog_body{
	width:100%;
	height:300px;
	background-color: white;
	overflow: auto;
}


.dialog_body>span{
	font-weight: 600; 
	margin-left: 50px; 
	font-size: 13px;
}


.dialog_body>input{
	background-color: #EEEEEE;
}

.dialog_foot{
	width:100%;
	height:50px;
	background-color: #F8F8F8;
}
.dialog_btn{
	width: 60px;
	height: 30px;
	border: 0px ;
	border-radius:5px;
	font-size: 14px;
	outline: none;
}
.shadow{
	width:100%;
	height:100%;
	position:absolute;
	left:0px;
	top:0px;
	z-index: 999;
	background-color:rgba(0,0,0,0.8);
}

.searchBar{
	width: 90%;
	height: 60px;
	font-size: 15px;
	background-color: #F2F2F2;
	border: 1px solid grey;
	line-height: 60px;
	padding-left: 30px;
}

.select{
	height:36px;
	width:150px;
	outline: none;
}
.shorttext{
	outline: none;
	font-size:12px;
	border:1px solid black;
	background-color:#FAFAFA;
	height:30px;
	width:50px;
}

.btn{
	height: 30px;
	width: 70px;
	background-color: #0B73DF;
	color: white;
	line-height: 30px;
	text-align: center;
	border-radius:5px;
	display: inline-block;
	cursor: pointer;
	margin-left: 240px;
}

.searchInfoTab{
	margin-top:20px;
	font-size: 14px;
	width:1130px;	
	border: 1px solid black;
}
.searchInfoTab>tr>td{
	border: 1px solid black;
}
.searchInfoTab>th{
	font-weight: bolder;
}
.searchInfoTab>tbody>tr>td{
	text-align:center;
	font-size:13px;
	color:black;
}

.btnToolBar{
	margin-top: 20px;	
}

#selectBtn{
	width:120px;
}

.btnToolBar>button{
	width:40px;
	height: 35px;
	background-color: white;
	outline: none;
	margin-right: 0px;
	
}

.optDiv{
	width:110px;
	background-color: wheat;
	display:none;
	position:relative;
	left:50px;
	top:5px;
	z-index:2;
}

.optUl{
	list-style:none; 
	border: 1px solid grey;
	margin: 0;
	padding: 0;
}
.optUl>li{
	border-bottom: 1px solid grey;
	text-align: center;	
	height: 40px;
	line-height: 40px;
	font-size:14px;
}
.optUl>li>a{
	text-decoration: none;
	color:black;
}

.timeText{
	outline: none;
	height: 25px;
	width: 110px;
	font-size: 15px;
}

