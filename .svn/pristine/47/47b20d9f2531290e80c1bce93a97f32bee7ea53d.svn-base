package pojo;

import java.util.Date;

/**
 * 房源跟进表
 * @author admin
 *
 */
public class HouseTrail {

	//房源跟进表id
	private int houseTrail_id;
	//跟进类型
	private int houseTrail_type;
	//跟进内容
	private String houseTrail_content;
	//跟进时间
	private Date houseTrail_time;
	//员工id
	private int staff_id;
	//房源id
	private int houseResource_id;
	//房源
	private HouseResource houseResource;
	//员工
	private Staff staff;
	
	
	public int getHouseTrail_id() {
		return houseTrail_id;
	}
	public void setHouseTrail_id(int houseTrail_id) {
		this.houseTrail_id = houseTrail_id;
	}
	public int getHouseTrail_type() {
		return houseTrail_type;
	}
	public void setHouseTrail_type(int houseTrail_type) {
		this.houseTrail_type = houseTrail_type;
	}
	public String getHouseTrail_content() {
		return houseTrail_content;
	}
	public void setHouseTrail_content(String houseTrail_content) {
		this.houseTrail_content = houseTrail_content;
	}
	public Date getHouseTrail_time() {
		return houseTrail_time;
	}
	public void setHouseTrail_time(Date houseTrail_time) {
		this.houseTrail_time = houseTrail_time;
	}
	public int getStaff_id() {
		return staff_id;
	}
	public void setStaff_id(int staff_id) {
		this.staff_id = staff_id;
	}
	public int getHouseResource_id() {
		return houseResource_id;
	}
	public void setHouseResource_id(int houseResource_id) {
		this.houseResource_id = houseResource_id;
	}
	public HouseResource getHouseResource() {
		return houseResource;
	}
	public void setHouseResource(HouseResource houseResource) {
		this.houseResource = houseResource;
	}
	public Staff getStaff() {
		return staff;
	}
	public void setStaff(Staff staff) {
		this.staff = staff;
	}
	
	
	
	
}
