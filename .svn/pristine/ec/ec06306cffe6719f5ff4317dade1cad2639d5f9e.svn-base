@CHARSET "UTF-8";

.searchBar{
	width: 90%;
	height: 60px;
	font-size: 15px;
	background-color: #F2F2F2;
	border: 1px solid grey;
	line-height: 60px;
	padding-left: 30px;
}

.select{
	height:36px;
	width:150px;
	outline: none;
}
.shorttext{
	outline: none;
	font-size:12px;
	border:1px solid black;
	background-color:#FAFAFA;
	height:30px;
	width:50px;
}

.btn{
	height: 30px;
	width: 70px;
	background-color: #0B73DF;
	color: white;
	line-height: 30px;
	text-align: center;
	border-radius:5px;
	display: inline-block;
	cursor: pointer;
	margin-left: 240px;
}

.searchInfoTab{
	margin-top:20px;
	font-size: 14px;
	width:1130px;
	border:1px solid grey;
}

.searchInfoTab>th{
	font-weight: bolder;
}
.searchInfoTab>tbody>tr>td{
	text-align:center;
	font-size:12px;
	color:black;
}