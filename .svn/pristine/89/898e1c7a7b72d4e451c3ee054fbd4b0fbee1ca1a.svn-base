package pojo;

import java.util.Date;

/**
 * 员工考勤表
 * @author admin
 *
 */
public class CheckAttendance {
	
	//考勤表Id
	
	

	private int checkAttendance_id;
	//考勤日期
	private Date onDutyTime;
	//员工id
	private int staff_id;
	
	
	
	public int getCheckAttendance_id() {
		return checkAttendance_id;
	}
	public void setCheckAttendance_id(int checkAttendance_id) {
		this.checkAttendance_id = checkAttendance_id;
	}
	public Date getOnDutyTime() {
		return onDutyTime;
	}
	public void setOnDutyTime(Date onDutyTime) {
		this.onDutyTime = onDutyTime;
	}
	public int getStaff_id() {
		return staff_id;
	}
	public void setStaff_id(int staff_id) {
		this.staff_id = staff_id;
	}
	
	

}
