
.nav{
	
	width: 90%;
	height:40px;
	line-height: 40px;
	font-size: 18px;
	font-weight:600;
	background-color: #F2F2F2;
	padding-left:20px; 
	margin-bottom: 20px;
}

.list>li{
	list-style: none;
	line-height:50px;
	
}
